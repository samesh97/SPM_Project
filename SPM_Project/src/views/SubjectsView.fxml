<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="507.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.controllers.SubjectsViewController">
   <children>
      <TextField fx:id="search_Subject" layoutX="36.0" layoutY="51.0" prefHeight="25.0" prefWidth="466.0" text="enter name" />
      <Button fx:id="btn_SearchSubject" layoutX="502.0" layoutY="51.0" mnemonicParsing="false" text="search" />
      <TableView fx:id="table_ViewSubjects" editable="true" layoutX="15.0" layoutY="163.0" prefHeight="259.0" prefWidth="571.0">
        <columns>
          <TableColumn prefWidth="50.0" text="Subject Code" />
          <TableColumn prefWidth="37.0" text="Name" />
            <TableColumn minWidth="5.0" prefWidth="58.0" text="Offered Year" />
            <TableColumn prefWidth="70.0" text="Offered Semester" />
            <TableColumn prefWidth="71.0" text="Lecture Hrs" />
            <TableColumn prefWidth="75.0" text="Tutorial Hrs" />
            <TableColumn prefWidth="61.0" text="Lab Hrs" />
            <TableColumn prefWidth="39.0" text="Evaluation Hrs" />
            <TableColumn prefWidth="49.0" text="Update" />
            <TableColumn prefWidth="64.0" text="Delete" />
        </columns>
      </TableView>
      <Button fx:id="btn_AddNewSubject" layoutX="19.0" layoutY="422.0" mnemonicParsing="false" onAction="#onAddNewSubjectClicked" prefHeight="25.0" prefWidth="269.0" text="Add new Subject ">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btn_ViewAllSubjectsView" layoutX="298.0" layoutY="422.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="283.0" text="View All Subjects">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
