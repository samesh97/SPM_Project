<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="507.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.controllers.SubjectsViewController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="102.0" prefWidth="601.0">
               <children>
                  <TextField fx:id="search_Subject" prefHeight="25.0" text="enter name">
                     <HBox.margin>
                        <Insets top="50.0" />
                     </HBox.margin>
                     <padding>
                        <Insets right="150.0" />
                     </padding>
                  </TextField>
                  <Button fx:id="btn_SearchSubject" mnemonicParsing="false" text="search">
                     <HBox.margin>
                        <Insets top="50.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets left="120.0" right="120.0" />
               </VBox.margin>
            </HBox>
            <TableView fx:id="table_ViewSubjects" editable="true" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="259.0" prefWidth="580.0">
              <columns>
                <TableColumn minWidth="-Infinity" prefWidth="39.0" text="Subject Code" />
                <TableColumn minWidth="-Infinity" prefWidth="50.0" text="Name" />
                  <TableColumn minWidth="-Infinity" prefWidth="46.0" text="Offered Year" />
                  <TableColumn minWidth="-Infinity" prefWidth="44.0" text="Offered Semester" />
                  <TableColumn minWidth="-Infinity" prefWidth="71.0" text="Lecture Hrs" />
                  <TableColumn minWidth="-Infinity" prefWidth="75.0" text="Tutorial Hrs" />
                  <TableColumn prefWidth="61.0" text="Lab Hrs" />
                  <TableColumn prefWidth="71.0" text="Evaluation Hrs" />
                  <TableColumn prefWidth="54.0" text="Update" />
                  <TableColumn minWidth="-Infinity" prefWidth="50.0" text="Delete" />
              </columns>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="30.0" />
               </VBox.margin>
            </TableView>
            <HBox prefHeight="59.0" prefWidth="601.0">
               <children>
                  <Button fx:id="btn_AddNewSubject" mnemonicParsing="false" onAction="#onAddNewSubjectClicked" prefHeight="25.0" prefWidth="269.0" text="Add new Subject ">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btn_ViewAllSubjectsView" mnemonicParsing="false" prefHeight="25.0" prefWidth="283.0" text="View All Subjects">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
