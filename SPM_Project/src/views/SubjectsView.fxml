<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="507.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.controllers.SubjectsViewController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <AnchorPane prefHeight="202.0" prefWidth="601.0">
               <children>
                  <TextField fx:id="search_Subject" prefHeight="25.0" text="enter name" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="200.0" />
                  <Button fx:id="btn_SearchSubject" layoutX="401.0" mnemonicParsing="false" text="search" AnchorPane.rightAnchor="150.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
      <TableView fx:id="table_ViewSubjects" editable="true" layoutX="15.0" layoutY="163.0" prefHeight="259.0" prefWidth="571.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0">
        <columns>
          <TableColumn minWidth="-Infinity" prefWidth="39.0" text="Subject Code" />
          <TableColumn prefWidth="36.0" text="Name" />
            <TableColumn minWidth="5.0" prefWidth="46.0" text="Offered Year" />
            <TableColumn prefWidth="44.0" text="Offered Semester" />
            <TableColumn prefWidth="71.0" text="Lecture Hrs" />
            <TableColumn prefWidth="75.0" text="Tutorial Hrs" />
            <TableColumn prefWidth="61.0" text="Lab Hrs" />
            <TableColumn prefWidth="71.0" text="Evaluation Hrs" />
            <TableColumn prefWidth="54.0" text="Update" />
            <TableColumn prefWidth="73.0" text="Delete" />
        </columns>
      </TableView>
      <Button fx:id="btn_AddNewSubject" layoutX="15.0" layoutY="444.0" mnemonicParsing="false" onAction="#onAddNewSubjectClicked" prefHeight="25.0" prefWidth="269.0" text="Add new Subject ">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btn_ViewAllSubjectsView" layoutX="301.0" layoutY="444.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="283.0" text="View All Subjects">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
